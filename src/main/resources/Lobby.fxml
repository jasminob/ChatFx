<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="lobbyAnchorPane" minHeight="400.0" minWidth="400.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Lobby">
   <children>
      <TextArea fx:id="sentMsg" editable="false" prefHeight="200.0" prefWidth="220.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <TextField fx:id="sendMsg" prefHeight="25.0" prefWidth="155.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="200.0" />
      <Button fx:id="send" layoutX="155.0" layoutY="200.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="31.0" prefWidth="66.0" text="Send" AnchorPane.leftAnchor="155.0" AnchorPane.topAnchor="200.0" />
      <ListView fx:id="userList" layoutX="220.0" prefHeight="200.0" prefWidth="180.0" AnchorPane.leftAnchor="220.0" AnchorPane.topAnchor="0.0" />
      <Button fx:id="exitBtn" layoutX="356.0" layoutY="200.0" mnemonicParsing="false" onAction="#onExit" text="Exit" />
      <Label fx:id="appMsg" layoutX="5.0" layoutY="231.0" prefHeight="21.0" prefWidth="220.0" />
   </children>
</AnchorPane>
